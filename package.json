{
  "name": "contact-manager-test",
  "version": "1.0.0",
  "description": "Demonstration to convert Backbone.js application to React + Redux",
  "main": "index.js",
  "scripts": {
    "postinstall": "node ./script/build.js",
    "start": "node ./app/js/server.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js ./app/js/server.js",
    "watch": "node ./script/compile.js",
    "test": "mocha --compilers js:babel-core/register --watch $(find app -name '*test.js') -R spec"
  },
  "keywords": [
    "redux",
    "react",
    "contactmanager",
    "test"
  ],
  "author": "Jirat Ki. (n3tr)",
  "license": "ISC",
  "dependencies": {
    "backbone": "^1.2.3",
    "express": "^4.13.3",
    "jquery": "^2.1.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "redux": "^3.0.4",
    "underscore": "^1.8.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "envify": "^3.4.0",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "expect": "^1.13.0",
    "mocha": "^2.3.4"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
